    "@storybook/addon-viewport": "^5.3.19",
    "@storybook/react": "^5.3.19",
    "babel-loader": "^8.1.0",
    "babel-plugin-transform-node-env-inline": "^0.4.3",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "chai": "^4.2.0",
    "clsx": "^1.1.1",
    "css-loader": "^3.5.3",
    "date-fns": "^2.14.0",
    "eslint": "^6.6.0",
    "eslint-config-nebenan": "^1.3.2",
    "lodash": "^4.17.15",
    "mocha": "^8.0.1",
    "nebenan-helpers": "^4.2.0",
    "nebenan-ui-kit": "^4.12.0",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "rollup": "^2.15.0",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-peer-deps-external": "^2.2.2",
    "rollup-plugin-postcss": "^3.1.2",
    "sass": "^1.26.8",
    "sass-functions": "^1.4.0",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.2.1"
  },
  "dependencies": {},
  "peerDependencies": {
    "nebenan-helpers": "^3.6.0 || ^4.2.0",
    "lodash": "^4.17.15",
    "react": "^16.13.1",
    "date-fns": "^2.14.0",
    "prop-types": "^15.7.2",
    "clsx": "^1.1.1",
    "nebenan-ui-kit": "^3.0.0 || ^4.11.0",
    "@babel/runtime": "^7.10.2"
  }
