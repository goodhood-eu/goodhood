  "license": "SEE LICENSE IN LICENSE FILE",
  "homepage": "https://goodhood.eu/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/goodhood-eu/goodhood.git"
  },
  "bugs": {
    "url": "https://github.com/goodhood-eu/goodhood/issues"
  },
  "version": "0.0.1-beta.7",
  "module": "lib/index.esm.js",
  "main": "lib/index.js",
  "sideEffects": false,
  "scripts": {
    "reinstall": "rm -rf node_modules package-lock.json && npm install",
    "prepublishOnly": "npm run check && npm run build",
    "check": "npm run lint && npm run test",
    "lint": "npx eslint -f codeframe \"./**/*.{js,jsx}\"",
    "build": "npx rollup --config rollup.config.js",
    "start": "npx start-storybook -p 3000 --ci",
    "test": "NODE_ENV=test TZ='GMT' mocha \"./{,!(node_modules)/**}/*.test.{js,jsx}\"",
    "test:tofile": "NODE_ENV=test TZ='GMT' mocha --no-colors -R spec \"./{,!(node_modules)/**}/*.test.{js,jsx}\" 2>&1 | tee test-report.log"
  },
  "devDependencies": {
    "@babel/register": "^7.10.1",
    "@babel/runtime": "^7.10.2",
    "chai": "^4.2.0",
    "clsx": "^1.1.1",
    "date-fns": "^2.14.0",
    "mocha": "^8.0.1",
    "nebenan-helpers": "^4.2.0",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "dependencies": {},
  "peerDependencies": {
    "nebenan-helpers": "^3.6.0 || ^4.2.0",
    "lodash": "^4.17.15",
    "react": "^16.13.1",
    "date-fns": "^2.14.0",
    "prop-types": "^15.7.2",
    "clsx": "^1.1.1",
    "nebenan-ui-kit": "^3.0.0 || ^4.11.0",
    "@babel/runtime": "^7.10.2"
  }
}
